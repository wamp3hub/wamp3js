{
  "name": "wamp3js",
  "version": "0.4.1",
  "description": "JavaScript library for WAMP3",
  "author": "WAMP3Hub",
  "license": "MIT",
  "homepage": "https://wamp3hub.github.io/documentation/",
  "repository": {
    "type": "git",
    "url": "https://github.com/wamp3hub/wamp3js.git"
  },
  "bugs": {
    "url": "https://github.com/wamp3hub/wamp3js/issues"
  },
  "keywords": ["wamp3"],
  "type": "module",
  "main": "./build/index.umd.cjs",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.umd.cjs"
    }
  },
  "files": [
    "build"
  ],
  "scripts": {
    "local": "vite",
    "test": "vitest --coverage",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish:release": "pnpm pack --dry-run && pnpm publish"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/uuid": "^9.0.7",
    "@vitest/coverage-v8": "^1.0.4",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "uuid": "^9.0.1"
  }
}
